.calender-container {
  width: 100%;
  height: auto;
  overflow: hidden;
}
.calender-header {
  width: 100%;
  font-size: 34rpx;
  text-indent: 60rpx;
  color: #332c2b;
  margin: 50rpx 0 30rpx 0;
}
.calender-body {
  width: 742rpx;
  margin: 0 auto;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.calender-date {
  font-size: 38rpx;
  width: 106rpx;
  height: auto;
  margin-bottom: 30rpx;
  text-align: center;
  font-weight: bold;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
.calender-disable {
  color: #a1a1a1;
  text-decoration: line-through;
}